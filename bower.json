{
  "name": "DragNDoc",
  "repo": "novapost/dragndoc",
  "version": "0.0.1",
  "ignore": [
    "**/.*",
    "node_modules",
    "components"
  ],
  "dependencies": {
    "jquery": "~2.0.3",
    "backbone": "~1.1.0",
    "underscore": "~1.5.2",
    "bootstrap": "~3.0.3",
    "backbone.bootstrap-modal": "https://github.com/coulix/backbone.bootstrap-modal/archive/bootstrap3.zip",
    "font-awesome": "~4.0.3"
  },

  "overrides": "Fixing the lack of main property on some bower packages, see https://github.com/paulmillr/read-components",
  "overrides": {
    "backbone": {
      "main": "backbone.js"
    },
    "underscore": {
      "main": "underscore.js"
    },
    "font-awesome": {
      "main": ["fonts", "css"]
    },
    "backbone.bootstrap-modal": {
      "main": ["src"]
    }
  }
}